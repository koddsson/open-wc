{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "esnext",
    "declaration": true,
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom"],
    "checkJs": true,
    "isolatedModules": true
  },
    "files": [
      "./index.ts",
      "./observable-map.ts",
      "./context-protocol.ts"
    ],
  "isolatedModules": true,
}
